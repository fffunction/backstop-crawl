    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "xo": {
    "extends": "prettier"
  },
  "keywords": [
    "backstopjs",
    "backstop",
    "crawl",
    "crawler",
    "config",
    "generator",
    "regression",
    "test"
  ],
  "files": [
    "lib",
    "index.js"
  ],
  "author": {
    "name": "Dan Reeves",
    "email": "danr@fffunction.co",
    "url": "https://fffunction.co"
  },
  "contributors": [
    {
      "name": "Dan Reeves",
      "email": "danr@fffunction.co",
      "url": "https://fffunction.co"
    }
  ],
  "bugs": {
    "url": "https://github.com/fffunction/backstop-crawl/issues",
    "email": "danr@fffunction.co"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "1.1.3",
    "cli-truncate": "1.0.0",
    "jsonfile": "^3.0.0",
    "meow": "3.7.0",
    "mkpath": "1.0.0",
    "object.entries": "1.0.4",
    "ora": "^1.3.0",
    "simplecrawler": "^1.1.3",
    "update-notifier": "^2.2.0",
    "url-parse": "^1.1.9",
    "valid-url": "1.0.9"
