    "coveralls": "^2.13.1",
    "eslint-config-prettier": "^2.2.0",
    "execa": "^0.7.0",
    "live-server": "1.2.0",
    "nyc": "^11.0.3",
    "path-exists": "3.0.0",
    "pify": "^3.0.0",
