{
  "name": "backstop-crawl",
  "version": "2.2.0",
  "description": "Crawl a site to generate a backstopjs config",
  "repository": "https://github.com/fffunction/backstop-crawl",
  "homepage": "https://github.com/fffunction/backstop-crawl",
  "main": "index.js",
  "bin": {
    "backstop-crawl": "./index.js"
  },
  "scripts": {
    "test": "xo && nyc ava",
